<div class="container">
  <header></header>
  <div class="col-md-8 col-md-offset-2">
    <h2 class="card center">Create User</h2>
    <div class="content">
      <form class="card" method="post" action="/users">
	       <p>Username: <input type="text" name="user[username]"></p>
         <p>Password: <input type="password" name="user[password]"></p>
         <p>First Name: <input type="text" name="user[first_name]"></p>
         <p>Last Name: <input type="text" name="user[last_name]"></p>
         <p>Age: <input type="text" name="user[age]"></p>
	       <input type="submit" value="Create Account">
	   </form>
    </div>
  </div>
</div>
